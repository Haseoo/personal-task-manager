<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity" stylesheets="@dialog.css" vgap="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints vgrow="SOMETIMES" />
    <RowConstraints valignment="CENTER" vgrow="NEVER" />
  </rowConstraints>
   <children>
      <TableView fx:id="tagTable" prefHeight="300.0" prefWidth="400.0">
        <columns>
          <TableColumn prefWidth="292.79998779296875" text="Tag name" />
          <TableColumn prefWidth="107.20001220703125" text="Task count" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <GridPane hgap="5.0" GridPane.rowIndex="1">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" percentWidth="33.0" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" percentWidth="34.0" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" percentWidth="33.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button mnemonicParsing="false" styleClass="btnLarge" text="Add" />
            <Button mnemonicParsing="false" styleClass="btnLarge" text="Remove" GridPane.columnIndex="2" />
            <Button fx:id="onTagEdit" mnemonicParsing="false" styleClass="btnLarge" text="Edit" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
         </children>
      </GridPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
